<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="600.0" styleClass="root" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="GUI.dashboard.DashboardController">
	<children>
		<Pane>
			<children>
			
				<ImageView fitHeight="60" fitWidth="60" layoutX="5" layoutY="5" pickOnBounds="true" preserveRatio="true">

					

				</ImageView>

				<Text layoutX="75.0" layoutY="25.0" text="Username">
					<font>
						<Font name="System Bold" size="14.0" />
					</font>
				</Text>

				<Text layoutX="75.0" layoutY="50.0" text="Location" />

			</children>
		</Pane>

		<SplitPane dividerPositions="0.2" focusTraversable="true" VBox.vgrow="ALWAYS">
			<items>
				<VBox prefHeight="100" prefWidth="100" spacing="5.0">
					<padding>
						<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
					</padding>

					<Button id="btn_dashboard" mnemonicParsing="false" onAction="#handleShowDashboard" styleClass="positiveButton" text="DashBoard" />

					<Button layoutY="28.0" mnemonicParsing="false" onAction="#handleAddTrans" styleClass="positiveButton" text="Add Transport" />

					<Button layoutY="56.0" mnemonicParsing="false" onAction="#handleShowCustomer" styleClass="positiveButton" text="Show Customer" />

					<Pane prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />

					<Button id="btn_logout" onAction="#handleLogout" styleClass="negativeButton" text="Logout" />

				</VBox>
				<StackPane prefHeight="150.0" prefWidth="200.0">
					<children>
						<VBox prefHeight="200.0" prefWidth="100.0">
							<children>
								<TextField id="searchValue" promptText="Search" />
                        <TableView fx:id="carList" prefHeight="282.0" prefWidth="475.0">
                          <columns>
                            <TableColumn fx:id="carName" prefWidth="75.0" text="Name" />
                            <TableColumn fx:id="carModel" prefWidth="75.0" text="Model" />
                              <TableColumn fx:id="carPrice" prefWidth="75.0" text="Price" />
                          </columns>
                        </TableView>
							</children>
						</VBox>
					</children>
				</StackPane>
			</items>
		</SplitPane>
		<HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
			<children>
				<Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Copyright" HBox.hgrow="ALWAYS">
					<font>
						<Font size="11.0" fx:id="x3" />
					</font>
					<textFill>
						<Color blue="0.625" green="0.625" red="0.625" fx:id="x4" />
					</textFill>
				</Label>
				<Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
				<Label font="$x3" maxWidth="-1.0" text="Maharishi University of Management" textFill="$x4" HBox.hgrow="NEVER" />
			</children>
			<padding>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</padding>
		</HBox>
	</children>

	<stylesheets>
		<URL value="@DashBoard.css" />
	</stylesheets>

</VBox>
